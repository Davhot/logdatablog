$('#images_container').html("<%= j render 'load_editor_image' %>")

$(".set-article-image").click(function (e) {
  var address = $(this).data('address');
  var title = $(this).data('title');
  $("#editormd-image-dialog-address-input").val(address)
  $("#editormd-image-dialog-title-input").val(title)
  e.preventDefault();
})
